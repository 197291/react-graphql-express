{
  "name": "react-graphql-recipes",
  "version": "0.1.0",
  "description": "A starter for React-Apollo-GraphQL projects",
  "main": "server.js",
  "scripts": {
    "server": "nodemon --exec babel-node ./server/index.js",
    "build": "mkdir build && babel -d ./build ./server -s",
    "start": "node ./build/index.js",
    "clean": "rm -rf build",
    "test": "echo test",
    "prestart": "npm run clean && npm run build",
    "client": "cd client && npm start",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client",
    "format": "prettier --write \"./client/src/**/*.{ts,tsx,scss,mdx,md,js,json,html,yaml,yml}\"",
    "dev": "concurrently --names \"server,client\" \"npm run server --silent\" \"npm run client --silent\""
  },
  "engines": {
    "node": "11.10.0",
    "npm": "6.9.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "apollo-server-express": "^1.4.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cors": "^2.8.5",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.20"
  },
  "devDependencies": {
    "nodemon": "^1.17.5",
    "prettier": "^1.17.1"
  }
}
